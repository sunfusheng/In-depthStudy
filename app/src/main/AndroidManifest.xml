    <uses-permission android:name="android.permission.CAMERA"/>
    <uses-permission android:name="android.permission.READ_LOGS" />

    <application
        android:allowBackup="true"
        android:name=".MainApplication"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >

        <meta-data
            android:name="com.amap.api.v2.apikey"
            android:value="89f4f435727d68fa30c02063b140e81a"/>

        <activity
            android:name=".ui.activity.MainActivity"
            android:label="@string/app_name"
            android:launchMode="singleInstance">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <!-- 崩溃信息 -->
        <activity android:name=".ui.activity.CrashActivity"
                  android:screenOrientation="portrait"/>

        <!-- OkHttp -->
        <activity android:name=".ui.activity.OkHttpActivity"
                  android:screenOrientation="portrait"/>

        <!-- Retrofit -->
        <activity android:name=".ui.activity.RetrofitActivity"
                  android:screenOrientation="portrait"/>

        <!-- 高德地图 -->
        <activity android:name=".ui.activity.AmapActivity"
                  android:screenOrientation="portrait"/>

        <!-- RxJava -->
        <activity android:name=".ui.activity.rxjava.RxJava1Activity"
                  android:screenOrientation="portrait"/>
        <activity android:name=".ui.activity.rxjava.RxJava2Activity"
                  android:screenOrientation="portrait"/>
        <activity android:name=".ui.activity.rxjava.RxJava3Activity"
                  android:screenOrientation="portrait"/>
        <activity android:name=".ui.activity.rxjava.RxJava4Activity"
                  android:screenOrientation="portrait"/>
        <activity android:name=".ui.activity.rxjava.RxJava5Activity"
                  android:screenOrientation="portrait"/>
        <activity android:name=".ui.activity.rxjava.RxJava6Activity"
                  android:screenOrientation="portrait"/>
        <activity android:name=".ui.activity.rxjava.RxJava7Activity"
                  android:screenOrientation="portrait"/>
        <activity android:name=".ui.activity.rxjava.RxJava8Activity"
                  android:screenOrientation="portrait"/>

        <!-- 购物车 -->
        <activity android:name=".ui.activity.ShoppingViewActivity"
                  android:screenOrientation="portrait"/>

        <!-- Fresco -->
        <activity android:name=".ui.activity.FrescoActivity"
                  android:screenOrientation="portrait"/>

        <!-- 多线程 -->
        <activity android:name=".ui.activity.MultiThreadActivity"
                  android:screenOrientation="portrait"/>

        <!-- 线程 -->
        <activity android:name=".ui.activity.ThreadActivity"
                  android:screenOrientation="portrait"/>

        <!-- 线程池 -->
        <activity android:name=".ui.activity.ThreadPoolActivity"
                  android:screenOrientation="portrait"/>

        <!-- 反射 -->
        <activity android:name=".ui.activity.ReflectionActivity"
                  android:screenOrientation="portrait"/>

        <!-- 注解 -->
        <activity android:name=".ui.activity.AnnotationActivity"
                  android:screenOrientation="portrait"/>

        <!-- ShapeView -->
        <activity android:name=".ui.activity.ShapeViewActivity"
                  android:screenOrientation="portrait"/>

        <!-- MarqueeView -->
        <activity android:name=".ui.activity.MarqueeViewActivity"
                  android:screenOrientation="portrait"/>

        <!-- ShareView -->
        <activity android:name=".ui.activity.ShareViewActivity"
                  android:screenOrientation="portrait"/>

        <!-- IntentService -->
        <service android:name=".service.MyIntentService"/>

        <!-- 高德定位服务 -->
        <service android:name="com.amap.api.location.APSService"/>

    </application>

</manifest>
